// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model WebAccount {
  userId String @unique
}

model Product {
  id  Int @unique @default(autoincrement())
  name String
  description String
  image String
  price Int @default(0)
}